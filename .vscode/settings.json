{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.bk": true,
        "**/node_modules": true,
        "**/target": true
    },
    "[rust]": {
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.formatOnSave": true
    },
    "editor.formatOnSave": true,
    "git.ignoreLimitWarning": true,
    "git.autoRepositoryDetection": false,
    "eslint.enable": false,
    "rust-analyzer.checkOnSave.command": "clippy",
}